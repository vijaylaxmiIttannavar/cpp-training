#include <iostream>
using namespace std;

void fun(int matrix[], int m, int n)
{
    int c=0;
    int s=0;
   for(int i=0;i<m;i++)
   {
       for(int j=0;j<n;j++)
       { 
               //if(i==m-1)
           {
               //matrix[i]=0;
               i==c;
               s=s+matrix[i];
           }
        }
        c++;
   }
    cout<<s<<endl;
}

int main()
{
    int m=4, n=5;
    int matrix[m*n]={ 1, 2, 3, 4, 5, 
                    11, 12, 13, 14, 15,
                    21, 22, 23, 24, 25,
                    31, 32, 33, 34, 35 };
    fun(matrix, m, n);
}
