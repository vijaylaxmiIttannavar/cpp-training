#include <iostream>

using namespace std;

void fun(char *str)
{
    int i=0, c=0;
    while(str[i]!='\0')
    {
        if(str[i]!='\t')
        {
             cout<<str[i]; 
        }
        i++;
    }
}

int main()
{
    char str[50];
   cin.getline(str, 50);
    fun(str);
}
